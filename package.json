    "url": "http://outa.im/"
  },
  "repository": "outaTiME/outa.im",
  "license": "MIT",
  "engines": {
    "node": ">=0.12.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "browser-sync": "^2.18.0",
    "dateformat": "^2.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.0",
    "gulp-cache": "^0.4.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-eslint": "^3.0.0",
    "gulp-filter": "^4.0.0",
    "gulp-gh-pages": "^0.5.0",
    "gulp-header": "^1.8.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.1.0",
    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-replace-task": "^0.11.0",
    "gulp-rev": "^7.1.0",
    "gulp-rev-napkin": "^0.1.0",
    "gulp-rev-replace": "^0.4.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.9.0",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.1.0",
    "main-bower-files": "^2.13.0",
    "run-sequence": "^1.2.0",
    "wiredep": "^4.0.0"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jquery": true
    },
    "rules": {
      "no-console": "off",
      "comma-dangle": [
        "error",
        "never"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "space-before-function-paren": [
        "warn",
        "never"
      ]
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "eslint:recommended"
  }
}
